{
  "name": "Foodonthestove",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "android:assemble": "cd android && ./gradlew clean assembleRelease  && cd ..",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* &&  rm -rf node_modules/ &&  yarn cache clean",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "ios:bundle": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios/assets"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.5",
